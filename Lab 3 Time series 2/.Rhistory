knitr::opts_chunk$set(echo = TRUE)
rm(list=ls())
library('forecast')
library('tseries')
data=read.csv("soya_oil data set for TS forecast.csv")
class(data) # dataframe
data=ts(data$Dollar,start=1,end=128,frequency=4)
class(data) # time series
# View(data)
plot(data,xlab='Years',ylab='Soya Oil sales')
dd <- diff(data)
plot(dd,ylab='Differenced Soya Oil Sales')
plot(data,xlab='time',ylab='Soya Oil sales')
dd <- diff(data)
plot(dd,ylab='Differenced Soya Oil Sales')
ld <- log10(data)
plot(ld,ylab='Differenced Log (Soya Oil Sales)')
ld <- log10(data)
dld <- diff(ld)
plot(dld,ylab='Differenced Log (Soya Oil Sales)')
acf(data)
acf(data)
pacf(data)
adf.test(data)
par(mfrow=c(1,2))
acf(ts(dld),main="ACF Soya Oil Sales")
pacf(ts(dld),main="PACF Soya Oil Sales")
par(mfrow=c(1,2))
acf(ts(data),main="ACF Soya Oil Sales")
pacf(ts(data),main="PACF Soya Oil Sales")
ARIMAfit=auto.arima(dld,ic="aic",trace=TRUE)
summary(ARIMAfit)
ARIMAfit=auto.arima(data,ic="aic",trace=TRUE)
summary(ARIMAfit)
fc=forecast(ARIMAfit,level=c(95),h=3*12)
plot(fc)
summary(fc)
attributes(fc)
ARIMAfit=auto.arima(data,ic="aic",trace=TRUE)
summary(ARIMAfit)
